{
  "configuration": {
    "appenders": {
      "Console": {
        "name": "STDOUT",
        "PatternLayout": {
          "pattern": "%d [%t] %-5level %logger{36} - %msg%n%throwable"
        }
      }
    },
    "loggers": {
      "root": {
        "level": "debug",
        "AppenderRef": {
          "ref": "STDOUT"
        }
      }
    }
  }
}